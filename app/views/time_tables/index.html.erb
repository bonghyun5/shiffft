<h1>Listing time_tables</h1>

<table>
  <tr>
    <th>Shift events</th>
    <th>Mon</th>
    <th>Tues</th>
    <th>Wed</th>
    <th>Thur</th>
    <th>Fri</th>
    <th>Sat</th>
    <th>Sun</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_tables.each do |time_table| %>
  <tr>
    <td><%= time_table.shift_events_id %></td>
    <td><%= time_table.mon %></td>
    <td><%= time_table.tues %></td>
    <td><%= time_table.wed %></td>
    <td><%= time_table.thur %></td>
    <td><%= time_table.fri %></td>
    <td><%= time_table.sat %></td>
    <td><%= time_table.sun %></td>
    <td><%= link_to 'Show', time_table %></td>
    <td><%= link_to 'Edit', edit_time_table_path(time_table) %></td>
    <td><%= link_to 'Destroy', time_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time table', new_time_table_path %>
